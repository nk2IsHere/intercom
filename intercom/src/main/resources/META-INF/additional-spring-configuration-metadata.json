{
  "properties": [
    {
      "name": "intercom.starterMode",
      "type": "eu.nk2.intercom.api.IntercomStarterMode",
      "description": "Intercom mode.",
      "defaultValue": "CLIENT_ONLY"
    },
    {
      "name": "intercom.host",
      "type": "java.lang.String",
      "description": "Intercom tcp socket host (applicable to client instances)."
    },
    {
      "name": "intercom.port",
      "type": "java.lang.Integer",
      "description": "Intercom tcp socket port (applicable to client instances)."
    },
    {
      "name": "intercom.serverSslSecurity",
      "type": "java.lang.Boolean",
      "description": "Enable SSL Security on server.",
      "defaultValue": false
    },
    {
      "name": "intercom.serverAllowWiretapping",
      "type": "java.lang.Boolean",
      "description": "Enable netty wiretapping on server.",
      "defaultValue": false
    },
    {
      "name": "intercom.clientSslSecurity",
      "type": "java.lang.Boolean",
      "description": "Enable SSL Security on client.",
      "defaultValue": false
    },
    {
      "name": "intercom.clientAllowWiretapping",
      "type": "java.lang.Boolean",
      "description": "Enable netty wiretapping on client.",
      "defaultValue": false
    }
  ]
}